<h1> Assign Permission for roles</h1>

<%= form_for @conn, page_path(@conn, :file_receive), [as: :session, multipart: true], fn f -> %>
<div class="form-group">
   <label for="" class="control-label">Video File</label>
   <%= file_input f, :roleid, class: "form-control" %>
</div>
<div class="form-group">
  <%= submit "Submit", class: "btn btn-primary" %>
</div>

<% end %>
